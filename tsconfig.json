{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./*"],
      "@root/*": ["./*"],
      "@src/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@modules/*": ["src/modules/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"]
    }
  }
}
